/**index.wxss**/
.container {
  background: #eee;
  display: flex;
  align-items: center;
  justify-content: center;
  height: 100vh;
}
.content {
  height: 10rem;
  width: 10rem;
  border-radius: 50%;
  background: #fff;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.scan-text {
  font-family: PingFangSC;
  font-weight: 400;
  font-size: 18px;
  color: rgb(16, 16, 16);
  font-style: normal;
  letter-spacing: 0px;
  line-height: 26px;
  text-decoration: none; 
}
.scanStyle {
  font-size: 160rpx;
}
.explain {
  font-family: PingFangSC;
  font-weight: 400;
  font-size: 32rpx;
  color: rgb(144, 147, 153);
  font-style: normal;
  letter-spacing: 0px;
  line-height: 46rpx;
  text-decoration: none;
  margin-top: 20rpx;
}
.mt30 {
  margin-top: 60rpx;
}
